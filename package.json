{
  "name": "turbodota-v4",
  "version": "4.0.0",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "dev:msw-server": "concurrently \"npx ts-node-esm src/mocks/server.ts\" \"npm run dev\"",
    "build": "npx prisma generate && vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "prisma": "npx prisma generate && npx prisma db push"
  },
  "devDependencies": {
    "@skeletonlabs/skeleton": "2.5.1",
    "@skeletonlabs/tw-plugin": "0.2.4",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.27.6",
    "@tailwindcss/forms": "0.5.7",
    "@tailwindcss/typography": "0.5.10",
    "@types/node": "20.10.2",
    "autoprefixer": "10.4.16",
    "concurrently": "^8.2.2",
    "msw": "^2.0.10",
    "postcss": "8.4.32",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "^3.0.0",
    "prisma": "^5.6.0",
    "supabase": "^1.115.4",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tailwindcss": "3.3.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.4.2",
    "vite-plugin-tailwind-purgecss": "0.1.4",
    "vitest": "^0.34.0",
    "vitest-mock-extended": "^1.3.1"
  },
  "type": "module",
  "dependencies": {
    "@flaticon/flaticon-uicons": "^3.0.0",
    "@floating-ui/dom": "1.5.3",
    "@lucia-auth/adapter-prisma": "^3.0.2",
    "@prisma/client": "^5.6.0",
    "@types/node-steam-openid": "^1.0.3",
    "@vercel/analytics": "^1.1.1",
    "dayjs": "^1.11.10",
    "dota2-css-hero-sprites": "^2.5.0",
    "lucia": "^2.7.4",
    "node-steam-openid": "^1.2.1"
  },
  "msw": {
    "workerDirectory": "static"
  }
}
